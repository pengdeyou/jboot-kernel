<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <groupId>org.jboot</groupId>
        <artifactId>jboot-kernel</artifactId>
        <version>${revision}</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>

    <artifactId>kernel-boot</artifactId>
    <name>${project.artifactId}</name>
    <packaging>jar</packaging>

    <properties>
        <module.name>org.jboot.kernel.boot</module.name>
    </properties>

    <dependencies>
        <!--Spring-->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-jdbc</artifactId>
            <exclusions>
                <exclusion>
                    <artifactId>tomcat-jdbc</artifactId>
                    <groupId>org.apache.tomcat</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-cache</artifactId>
        </dependency>
        <!-- J -->
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-launch</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-cloud</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-toolkit</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-secure</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-cache</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-log</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-swagger</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-tenant</artifactId>
        </dependency>
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-redis</artifactId>
        </dependency>
        <!--MyBatis-->
        <dependency>
            <groupId>org.jboot</groupId>
            <artifactId>kernel-mybatis</artifactId>
        </dependency>
        <dependency>
            <groupId>com.baomidou</groupId>
            <artifactId>mybatis-plus-spring-boot3-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.mybatis</groupId>
            <artifactId>mybatis-typehandlers-jsr310</artifactId>
        </dependency>
        <!-- Ehcache -->
        <dependency>
            <groupId>net.sf.ehcache</groupId>
            <artifactId>ehcache</artifactId>
        </dependency>
        <!-- Druid -->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>druid-spring-boot-3-starter</artifactId>
        </dependency>
        <!-- MySQL -->
        <dependency>
            <groupId>com.mysql</groupId>
            <artifactId>mysql-connector-j</artifactId>
        </dependency>
    </dependencies>

</project>
